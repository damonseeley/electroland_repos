package net.electroland.flash.pool{	public class PoolCreature{		public var creature:Creature;		private var creatureState:int = -91919;		private var creatureAccent:int = -91919;				public function PoolCreature(creature:Creature):void{			this.creature = creature;		}				public function setState(creatureState:int):void{			this.creatureState = creatureState;			if (creature){				creature.setState(creatureState);			}		}		public function getState():int{			return creatureState;		}		public function setAccent(creatureAccent:int):void{			this.creatureAccent = creatureAccent;			if (creature){				creature.setAccent(creatureAccent);			}		}		public function getAccent():int{			return creatureAccent;		}	}}