package net.electroland.flash.pool{		import flash.display.DisplayObject;		public interface Creature	{		// initialize the object, and return a DisplayObject.		// (any subclass probably wants to hold a global handle on the DisplayObject		// before returning it.		function init(env:Environment):DisplayObject;		// called only when the current state has changed.		// (this won't be useful unless you have the handle mentioned above :)		function setState(state:int);		// called every frame.		function setSpeedAndDepth(speed:Number, depth:Number);				// called only when eitan passes a non-null media source value		function activateVideo(src:String):void;		// called only when the current access has changed.		function setAccent(accent:int);	}}